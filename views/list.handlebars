<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>List shops</title>
  </head>
  <body>
    <form action='/avo/list'>
      <h2>List of all the shops</h2>

      <table>
        <tr>
          <th>Name Of shops</th>
        </tr>
        {{#each listOfShops}}
          <tr>
            <td>{{name}}</td>
          </tr>
        {{/each}}
      </table>
    </form>

  </body>
</html>